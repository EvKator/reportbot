{
    "compilerOptions": {
      "outDir": "dist",
      "sourceMap": true,
      "noImplicitAny": true,
      "module": "commonjs",
      "target": "es6",
      "allowJs": true,
      "lib":["es2015", "es2016", "es2017"],
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true
    },
    "include": ["./src/*"],
    "exclude": [
      "node_modules",
      "wwwroot"
    ]
    
  }